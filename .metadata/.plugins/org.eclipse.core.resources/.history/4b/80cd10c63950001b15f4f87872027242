package _2021_01_07_inherit;
/**
- Employee 클래스
  필드: 봉급(salary)
  메소드:
     봉급의 getter/setter
     getTax - 세금액(봉급의 10%)을 리턴
     봉급을 매개변수로 받아 초기화 하는 생성자
 */
public class Employee {
	//필드
	private int salary;

	//게터
	public int getSalary() {
		return salary;
	}

	//세터
	public void setSalary(int salary) {
		this.salary = salary;
	}
	
	//봉급을 매개변수로 받아 초기화 하는 생성자
	public int getTax(int salary) {
		int tax = (int)(salary*0.1);
		return tax;
	}
	

}
