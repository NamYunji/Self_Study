package _0111.member.vo;

import _0111.member.BonusPoint;

public class Vvip extends Member implements BonusPoint{
	
	
	public Vvip() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Vvip(String name, String grade, int point) {
		super(name, grade, point);
		// TODO Auto-generated constructor stub
	}

	@Override
	public double getBonus() {
		return this.getBonus()*0.15;
	}
	
	

}
